    <mapping class="com.jhmk.earlywaring.entity.SmModule"/>
    <mapping class="com.jhmk.earlywaring.entity.SmRole"/>
    <mapping class="com.jhmk.earlywaring.entity.SmRoleUser"/>
    <mapping class="com.jhmk.earlywaring.entity.SmUser"/>
      <mapping class="com.jhmk.earlywaring.entity.SmDept"/>
      <mapping class="com.jhmk.earlywaring.entity.SmEvaluate"/>
      <mapping class="com.jhmk.earlywaring.entity.SmHosptailLog"/>
      <mapping class="com.jhmk.earlywaring.entity.SmHosptailLog"/>
    <mapping class="com.jhmk.earlywaring.entity.SmHosptailLog"/>
    <mapping class="com.jhmk.earlywaring.entity.SmHosptailLog"/>
      <mapping class="com.jhmk.earlywaring.entity.SmDepts"/>
      <mapping class="com.jhmk.earlywaring.entity.SmUsers"/>
      <mapping class="com.jhmk.earlywaring.entity.DeptRel"/>
      <mapping class="com.jhmk.earlywaring.entity.SmRoleModule"/>
      <!-- <property name="connection.username"/> -->
    <!-- <property name="connection.password"/> -->

    <!-- DB schema will be updated if needed -->
    <!-- <property name="hbm2ddl.auto">update</property> -->
  </session-factory>
</hibernate-configuration>